<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>CraftFromContainers</AssemblyName>
    <Description>Aedenthorn.CraftFromContainers.FreeBuildFix.Plugin</Description>
    <Version>0.6.1.0</Version>
    <Product>$(Description)</Product>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RestoreAdditionalProjectSources>
      https://api.nuget.org/v3/index.json;
      https://nuget.bepinex.dev/v3/index.json;
      https://nuget.samboy.dev/v3/index.json
    </RestoreAdditionalProjectSources>
    <RootNamespace>CraftFromContainers</RootNamespace>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
    <LangVersion>latest</LangVersion>
    <Copyright>Copyright Â© 2023 By Aedenthorn</Copyright>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Askar0/Askar0-Plugins-ThePlanetCrafter.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Askar0/Askar0-Plugins-ThePlanetCrafter</PackageProjectUrl>
  </PropertyGroup>

  <Import Project="$(SolutionDir)\solution_private.targets" />
  <Import Project="$(SolutionDir)\solution_build.targets" />

</Project>
